    "def solution(n):\n",
    "    answer = ''\n",
    "    for i in range(1,n+1):\n",
    "        if i % 2 == 0:\n",
    "            answer += '박'\n",
    "        else:\n",
    "            answer += '수'\n",
    "    return answer\n",
    "\n",
    "print(solution(3))\n",
    "print(solution(7))\n",
    "print(solution(6))\n",
    "print(solution(16))"
   ]
